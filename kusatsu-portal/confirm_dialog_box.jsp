<div id="overlay"></div>
<div id="confirmDialogBox">
<table class="dialogBox" id="cdbox_table">
	<tr>
		<th class="dialogBox" id="cdbox_title"></th>
	</tr>
	<tr>
		<td class="dialogBox"><p id="cdbox_text"></p></td>
	</tr>
	<tr>
		<td class="dialogBoxButton">
			<input class="dialogBox" type="button" value="OK" onclick="hideConfirmDialogBox('Y')" />
			<input class="dialogBox" type="button" value="Cancel" onclick="hideConfirmDialogBox('N')" />
		</td>
	</tr>
</table>
</div>